{
  "capabilities": [
    {
      "browserName": "chrome",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "platform": "LINUX",
      "chromeOptions": {
        "args": ["--start-maximized"]
      }
    },
    {
      "browserName": "firefox",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "platform": "LINUX"
    },
    {
      "browserName": "MicrosoftEdge",
      "maxInstances": 5,
      "seleniumProtocol": "WebDriver",
      "platform": "WINDOWS"
    }
  ],
  "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
  "maxSession": 5,
  "register": true,
  "registerCycle": 5000,
  "hub": "http://localhost:4444",
  "nodePolling": 1800,
  "unregisterIfStillDownAfter": 60000,
  "downPollingLimit": 2,
  "debug": false
}
